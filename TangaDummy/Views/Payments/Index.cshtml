@model IEnumerable<TangaDummy.Payment>

@{
    ViewBag.Title = "Index";
}

<h2>Payments made thus far</h2>

<p>
   
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PaymentMade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaymentDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PermitType1.Permit_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PersonTable.Person_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PaymentMade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaymentDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PermitType1.Permit_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PersonTable.Person_Name)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.PaymentID }) |
        </td>
    </tr>
}

</table>
